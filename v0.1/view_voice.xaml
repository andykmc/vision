<Page
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="v0_1.view_voice"
	x:Name="Page"
	WindowTitle="Page"
	FlowDirection="LeftToRight"
	Width="640" Height="480"
	WindowWidth="640" WindowHeight="480" Loaded="Page_Loaded">
	<Page.Background>
		<SolidColorBrush Color="White" Opacity="0.9"/>
	</Page.Background>

	<Grid x:Name="LayoutRoot">
		<Image Margin="238,0,264,-38" Source="icons/mic_w.png" Stretch="Fill" VerticalAlignment="Bottom" Height="138" ClipToBounds="True"/>
		<Image Margin="238,-40,264,0" ClipToBounds="True" Source="icons\mic_w.png" Stretch="Fill" VerticalAlignment="Top" Height="138"/>
		<Image Margin="238,162,264,180" ClipToBounds="True" Source="icons\mic_w.png" Stretch="Fill"/>
	</Grid>
</Page>