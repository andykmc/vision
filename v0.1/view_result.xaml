			<Ellipse.Fill>
				<RadialGradientBrush GradientOrigin="-0.3,-0.3" RadiusX="1.5" RadiusY="1.5">
					<GradientStop Color="Black" Offset="1"/>
					<GradientStop Color="White"/>
				</RadialGradientBrush>
			</Ellipse.Fill>
		</Ellipse>
		<Ellipse x:Name="Image2" Margin="155,151,0,229" Stroke="Black" HorizontalAlignment="Left" Width="100" MouseDown="Image2_MouseDown">
			<Ellipse.Fill>
				<RadialGradientBrush GradientOrigin="-0.3,-0.3" RadiusX="1.5" RadiusY="1.5">
					<GradientStop Color="Black" Offset="1"/>
					<GradientStop Color="White"/>
				</RadialGradientBrush>
			</Ellipse.Fill>
		</Ellipse>
		<Ellipse x:Name="Image3" Margin="259,133,281,0" Stroke="Black" Height="100" VerticalAlignment="Top" MouseDown="Image3_MouseDown">
			<Ellipse.Fill>
				<RadialGradientBrush GradientOrigin="-0.3,-0.3" RadiusX="1.5" RadiusY="1.5">
					<GradientStop Color="Black" Offset="1"/>
					<GradientStop Color="White"/>
				</RadialGradientBrush>
			</Ellipse.Fill>
		</Ellipse>
		<Ellipse x:Name="Image4" Margin="0,151,177,229" Stroke="Black" HorizontalAlignment="Right" Width="100" MouseDown="Image4_MouseDown">
			<Ellipse.Fill>
				<RadialGradientBrush GradientOrigin="-0.3,-0.3" RadiusX="1.5" RadiusY="1.5">
					<GradientStop Color="Black" Offset="1"/>
					<GradientStop Color="White"/>
				</RadialGradientBrush>
			</Ellipse.Fill>
		</Ellipse>
		<Ellipse x:Name="Image5" Margin="0,198,73,182" Stroke="Black" HorizontalAlignment="Right" Width="100" MouseDown="Image5_MouseDown">
			<Ellipse.Fill>
				<RadialGradientBrush GradientOrigin="-0.3,-0.3" RadiusX="1.5" RadiusY="1.5">
					<GradientStop Color="Black" Offset="1"/>
					<GradientStop Color="White"/>
				</RadialGradientBrush>
			</Ellipse.Fill>
		</Ellipse>
		<TextBlock x:Name="ResultNameTextBox" Margin="309,0,331,182" TextWrapping="Wrap" VerticalAlignment="Bottom" FontSize="18.667" HorizontalAlignment="Center"/>
		<Button Content="Button" Margin="259,0,281,40" Style="{DynamicResource IMuse_button_style03}" VerticalAlignment="Bottom" Height="100" Width="100" Opacity="0.6" Name="rerankButton" Click="rerankButton_Click" />
		
	</Grid>
</Page>